<mapper namespace="imageboardSQL">

	<insert id="imageboardWrite" parameterType="iamgeboard.bean.ImageboardDTO">
		insert into imageboard values(seq_imageboard.nextval, #{imageid}, #{imagename}, #{imageprice}
		, #{imageqty}, #{imagecontent}, #{image1}, sysdate)
	</insert>

