package user.dao;

import user.bean.UserDTO;

public class UserDAO {
	public static UserDAO instance;
	
	public static UserDAO getInstance() {
		if(instance==null) {
			synchronized (UserDAO.class) {
				instance = new UserDAO();
			}	
		}
		return instance;
	}

